{
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run dev",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "http://backend-service/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}